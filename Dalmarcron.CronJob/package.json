  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/aws-lambda": "^8.10.145",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.9.0",
    "esbuild": "^0.24.0",
    "eslint": "^9.14.0",
    "prettier": "^3.3.3",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0"
  },
